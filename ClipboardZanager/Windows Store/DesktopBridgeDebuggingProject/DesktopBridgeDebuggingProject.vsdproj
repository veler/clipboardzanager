  <PropertyGroup>
    <BuildDependsOn>
      _CopyAppXFilesToPackageLayout;
    </BuildDependsOn>
  </PropertyGroup>
  <Target Name="_CopyAppXFilesToPackageLayout">
    <Error Condition="'@(LayoutFile)' != '' AND !Exists(%(LayoutFile.FullPath))" Text="LayoutFile does not exist [%(LayoutFile.FullPath)]" />
    <Copy Condition="Exists(%(LayoutFile.Identity))" SourceFiles="@(LayoutFile)" DestinationFiles="@(LayoutFile->'%(PackagePath)')" SkipUnchangedFiles="true" />
    <ItemGroup>
      <PdbToCopy Condition="Exists(%(LayoutFile.Identity))" Include="$([System.String]::Copy('%(LayoutFile.FullPath)').Replace('%(LayoutFile.Extension)','.pdb'))">
        <PackagePath>$([System.String]::Copy('%(LayoutFile.PackagePath)').Replace('%(LayoutFile.Extension)','.pdb'))</PackagePath>
      </PdbToCopy>
      <ConfigToCopy Condition="Exists(%(LayoutFile.Identity))" Include="$([System.String]::Copy('%(LayoutFile.FullPath)').Replace('%(LayoutFile.Extension)','.config'))">
        <PackagePath>$([System.String]::Copy('%(LayoutFile.PackagePath)').Replace('%(LayoutFile.Extension)','.config'))</PackagePath>
      </ConfigToCopy>
    </ItemGroup>
    <Copy Condition="Exists(%(PdbToCopy.Identity))" SourceFiles="@(PdbToCopy)" DestinationFiles="@(PdbToCopy->'%(PackagePath)')" SkipUnchangedFiles="true" />
    <Copy Condition="Exists(%(ConfigToCopy.Identity))" SourceFiles="@(ConfigToCopy)" DestinationFiles="@(ConfigToCopy->'%(PackagePath)')" SkipUnchangedFiles="true" />
  </Target>
  <Target Name="Build" DependsOnTargets="$(BuildDependsOn)" />
  <Target Name="Rebuild" DependsOnTargets="Build" />
  <Target Name="Clean" />
